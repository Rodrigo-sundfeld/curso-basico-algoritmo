algoritmo "SomaNumeros"
var
  N1, N2, SOMA: Inteiro

inicio
      Escreva("Por favor digite o primeiro número: ")
      Leia(N1)
      Escreva("Por favor digite o segundo número: ")
      Leia(N2)
      SOMA <- N1 + N2
      Escreva("A soma entre, ", N1, " e ", N2, " é igual a " , SOMA)
      
fimalgoritmo
