algoritmo "ProcedimentoRealizaSoma"

var
   X, Y: Inteiro

Procedimento Soma(A, B: Inteiro)
Inicio
      EscrevaL("Recebi o valor ", A)
      EscrevaL("Recebi o valor ", B)
      EscrevaL("A soma dos dois valores é ", A + B)
FimProcedimento

inicio
      X <- 5
      Y <- 3
      Soma(X, Y)
fimalgoritmo
