algoritmo "EstruturasLogicaRevisao"

var
   C1, C2: Inteiro
   
inicio
      C1 <- 1
      C2 <- 1

      Enquanto (C1 <=10) faca
               EscrevaL(C1)
               C1 <- C1 + 1
      FimEnquanto
      
      Repita
            EscrevaL(C2)
            C2 <- C2 + 1
      Ate (C2 > 10)

fimalgoritmo
