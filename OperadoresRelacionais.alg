algoritmo "OperadoresRelacionais"
var
   A, B, C: Inteiro
inicio
      A <- 2
      B <- 3
      C <- 5
      Escreval(A > B)
      Escreval(A = B)
      Escreval(A <> B)
      Escreval(A < 2)
      Escreval(A <= 2)
      Escreval(C = A + B)
      Escreval(C >= B^A)
      Escreval(B <= B%2)
      Escreval(1 = C%2)
fimalgoritmo
